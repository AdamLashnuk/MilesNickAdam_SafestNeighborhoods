import pandas as pd
import matplotlib.pyplot as plt

data_set = pd.read_excel("MonthlyCriminalActivity.xlsx")

#choosing column to read
col = "Zone"


#takes dataset, then column in dataset, then counts how many in dataset, then head how many unique zones to use.
crimeCounts = data_set[col].value_counts().tail(67)

#plotting
plt.title("Criminal activity")
plt.xlabel("Zone")
plt.ylabel("Count")

plt.style.use('fivethirtyeight')
crimeCounts.plot(kind="bar")
plt.show()
